    "   \"source\": [\n",
    "    \"class Solution:\\n\",\n",
    "    \"    def largestSquareArea(self, bottomLeft: List[List[int]], topRight: List[List[int]]) -> int:\\n\",\n",
    "    \"        length = len(bottomLeft)\\n\",\n",
    "    \"        max_side = 0\\n\",\n",
    "    \"        for i in range(length):\\n\",\n",
    "    \"            for j in range(i + 1, length):\\n\",\n",
    "    \"                ax1, ay1 = bottomLeft[i]\\n\",\n",
    "    \"                ax2, ay2 = topRight[i]\\n\",\n",
    "    \"                bx1, by1 = bottomLeft[j]\\n\",\n",
    "    \"                bx2, by2 = topRight[j]\\n\",\n",
    "    \"                w = min(ax2, bx2) - max(ax1, bx1)\\n\",\n",
    "    \"                h = min(ay2, by2) - max(ay1, by1)\\n\",\n",
    "    \"                side = min(w, h)\\n\",\n",
    "    \"                max_side = max(max_side, side)\\n\",\n",
    "    \"        return max_side ** 2\"\n",
